{
  "manifest_version": 3,
  "name": "Генератор Паролей",
  "version": "1.0",
  "description": "Генерация сложных паролей с копированием в буфер.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Генератор Паролей"
  },
  "permissions": ["clipboardWrite"],
  "icons": {
    "48": "icon.png"
  }
}
